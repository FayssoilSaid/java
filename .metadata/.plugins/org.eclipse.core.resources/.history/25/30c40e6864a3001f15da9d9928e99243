package metier;

public class Compte {
	private int code;
	protected float solde;
	
	public Compte(int c, float a) {
		code = c;
		solde = a;
	}
	
	public void verser(float mt) {
		solde+=mt;
	}
	
	public void retirer(float mt) {
		solde-=mt;
	}
	public String toString() {
		return "code = "+code+" solde = "+solde;
	}
}
