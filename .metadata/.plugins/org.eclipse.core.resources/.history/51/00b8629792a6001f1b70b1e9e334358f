package exercice5;

public class TestCompte {

	public static void main(String[] args) {
		Client clt1=new Client("Fayssoil","D007930U");
		Client clt2=new Client("Ammar","M007930K");
		
		Compte c1=new Compte(100,clt1);
		Compte c2=new Compte(200,clt2);
		System.out.println("------------------------");
		c1.afficher();
		c2.afficher();
		System.out.println("------------------------");
		System.out.println("Compte permuter");
		Compte.permuterComptes(c1, c2);
		c1.afficher();
		c2.afficher();

	}

}
