package exercice4;

public class Triangle {
	Point A,B,C;
	public Triangle(Point A,Point B,Point C) {
		this.A=A;
		this.B=B;
		this.C=C;
	}
	public Triangle(Triangle t) {
		this.A=t.A;
		this.B=t.B;
		this.C=t.C;
	}
	public void afficher() {
		System.out.println("Les sommets du triangle");
		this.A.afficher();
		this.B.afficher();
		this.C.afficher();
	}
}
